{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "images": {
    "minimumCacheTTL": 31536000,
    "formats": ["image/avif", "image/webp"]
  },
  "headers": [
    {
      "source": "/(.*\\.(webp|jpg|jpeg|png|gif|svg))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}